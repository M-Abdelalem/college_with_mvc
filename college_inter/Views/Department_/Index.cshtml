@model IEnumerable<DAl.Department>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<form>
    <input type="text" name="text" class="text-danger" />
    <input type="submit" value="search" />
</form>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.college.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.professor.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FK_Id_coullege)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FK_Id_professser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { Id = item.Id, name = item.name, FK_Id_professser = item.FK_Id_professser, FK_Id_coullege = item.FK_Id_coullege }) |
            @Html.ActionLink("Delete", "Delete", new { Id = item.Id, name = item.name, FK_Id_professser = item.FK_Id_professser, FK_Id_coullege = item.FK_Id_coullege })
        </td>
    </tr>
}

</table>
