@model IEnumerable<DAl.Section>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.professor.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.professor.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id, Time = item.Time,FK_ID_Course=item.FK_ID_Course,FK_Id_Professor=item.FK_Id_Professor }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id, Time = item.Time, FK_ID_Course = item.FK_ID_Course, FK_Id_Professor = item.FK_Id_Professor })
        </td>
    </tr>
}

</table>
