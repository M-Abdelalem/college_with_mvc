@model IEnumerable<DAl.Registration>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Section.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FK_Id_Student)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FK_Id_Section)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { Id = item.Id, FK_Id_Student = item.FK_Id_Student, FK_Id_Section = item.FK_Id_Section }) |
            @Html.ActionLink("Delete", "Delete", new { Id = item.Id, FK_Id_Student = item.FK_Id_Student, FK_Id_Section = item.FK_Id_Section })
        </td>
    </tr>
}

</table>
